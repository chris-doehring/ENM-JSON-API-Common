{
  "name": "chris-doehring/enm-json-api-common",
  "description": "Basic php implementation (shared structures for client and server) of the json api specification (jsonapi.org)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    },
    {
      "name": "Chris Doehring",
      "email": "info@chrisdoehring.de"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "guzzlehttp/psr7": "^1.4",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Enm\\JsonApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enm\\JsonApi\\Tests\\": "tests"
    }
  }
}
